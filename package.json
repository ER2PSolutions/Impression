{
  "name": "impression",
  "productName": "Impression",
  "version": "1.0.0",
  "description": "programme impression",
  "main": "main.js",
  "build": {
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "menuCategory": [
        "Startup"
      ],
      "runAfterFinish": false
    },
    "win": {
      "icon": "src/images/logo_er2p.png"
    },
    "appId": "com.er2p.impression"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dist":"build"
  },
  "keywords": [
    "electron"
  ],
  "author": "ER2P",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "electron": "^19.1.0",
    "electron-builder": "^23.3.3",
    "electron-pdf-window-s": "^2.1.0",
    "electron-rebuild": "^3.2.9"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "bootstrap": "^5.1.3",
    "devextreme": "22.1.4",
    "electron-dl": "^3.3.1",
    "electron-download-manager": "^2.1.2",
    "electron-fetch": "^1.7.4",
    "electron-pdf-window": "^1.0.12",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^5.2.1",
    "ext-ip": "^0.3.9",
    "externalip": "^1.0.2",
    "form-data": "^4.0.0",
    "jquery": "^3.6.0",
    "js-datepicker": "^5.18.0",
    "net": "^1.0.2",
    "node-fetch": "^3.2.10",
    "node-pre-gyp": "^0.17.0",
    "os": "^0.1.2",
    "pdf-to-printer": "^5.3.0",
    "pdf2json": "^2.0.1",
    "pdfjs-dist": "^2.16.105",
    "public-ip": "^4.0.0",
    "python": "^0.0.4",
    "toastr": "^2.1.4",
    "windows-1252": "^3.0.4",
    "xmlhttprequest": "^1.8.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "Impression"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
